Package: rshift
Type: Package
Title: Paleoecology Functions for Regime Shift Analysis
Version: 3.0.0
Authors@R: c(person("Alex H.", "Room",
                    role = c("aut", "cre", "cph"),
                    email = "alex.room@btinternet.com",
